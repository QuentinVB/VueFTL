{
  "name": "vue-ftl-server",
  "version": "1.0.0",
  "description": "backend for vue ftl project",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "test": "cross-env NODE_ENV=test mocha ./test/*",
    "pretest": "cross-env NODE_ENV=test npm run db:reset",
    "db:reset": "resetTestdb.bat && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all --seeders-path ./seeders/test",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create"
  },
  "type": "module",
  "author": "Quentin vanbutsele",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.4.1"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "color": "^4.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "esm": "^3.2.25",
    "express": "^4.17.3",
    "object-hash": "^2.2.0",
    "sequelize": "^6.17.0",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2"
  }
}
